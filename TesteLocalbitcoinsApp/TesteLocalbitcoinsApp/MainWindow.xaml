<Window x:Class="TesteLocalbitcoinsApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TesteLocalbitcoinsApp"
        xmlns:Spinners1="clr-namespace:xamlSpinnersWPF;assembly=xamlSpinnersWPF"
        mc:Ignorable="d"
        Title="LocalBitcoins" MinWidth="1450" MinHeight="720" Loaded="Window_Loaded" d:DesignWidth="1450" d:DesignHeight="720"
        Icon=".\Icon.ico">

    <Window.Resources>
        <!--<Storyboard x:Key="AnimateFlicker" RepeatBehavior="Forever">
            <DoubleAnimation Storyboard.TargetProperty="Opacity"
                     From="1"
                     To="0"
                     AutoReverse="True"
                             RepeatBehavior="Forever"/>
        </Storyboard>-->
        <Style x:Key="rowStyle" TargetType="DataGridRow">
            <Setter Property="Padding" Value="0" />
            <Setter Property="Margin" Value="0, -2" />
            <Setter Property="BorderThickness" Value="0, 1"/>
            <Setter Property="Background" Value="White" />
            <Setter Property="Height" Value="30" />
        </Style>
        <Style x:Key="TransparentStyle" TargetType="{x:Type Button}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border>
                            <Border.Style>
                                <Style TargetType="{x:Type Border}">
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Background" Value="#9DA9AD"/>
                                            <Setter Property="BorderThickness" Value="0"/>
                                            <Setter Property="CornerRadius" Value="20"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Border.Style>
                            <Grid Background="Transparent">
                                <ContentPresenter></ContentPresenter>
                            </Grid>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="Flat_Button" TargetType="{x:Type Button}">
            <Setter Property="Cursor" Value="Hand"/>
            <Setter Property="Foreground">
                <Setter.Value>
                    <SolidColorBrush Color="#777"/>
                </Setter.Value>
            </Setter>
            <Setter Property="Background" >
                <Setter.Value>
                    <SolidColorBrush Opacity="1" Color="Transparent" />
                </Setter.Value>
            </Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border x:Name="border" SnapsToDevicePixels="True" BorderThickness="0" Padding="0" BorderBrush="Transparent" CornerRadius="1" Background="{TemplateBinding Background}">
                            <Grid>
                                <ContentPresenter Margin="2" HorizontalAlignment="Center" VerticalAlignment="Center" RecognizesAccessKey="True" />
                            </Grid>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>

            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="true">
                    <Trigger.EnterActions>
                        <BeginStoryboard>
                            <Storyboard>
                                <ColorAnimation To="Black"
                                        Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)" 
                                        FillBehavior="HoldEnd" Duration="0:0:0.01" AutoReverse="False" RepeatBehavior="1x"/>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.EnterActions>

                    <Trigger.ExitActions>
                        <BeginStoryboard>
                            <Storyboard>
                                <ColorAnimation
                                            Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)" 
                                            FillBehavior="HoldEnd" Duration="0:0:0.01" AutoReverse="False" RepeatBehavior="1x"/>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.ExitActions>
                </Trigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>

    <Grid Margin="1" MinWidth="1440" HorizontalAlignment="Center">
        <Grid.RowDefinitions>
            <RowDefinition Height="0.5*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Margin="0" Grid.ColumnSpan="2" Panel.ZIndex="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1.2*"/>
                <ColumnDefinition Width="2.5*" />
                <ColumnDefinition Width="1.2*" />
            </Grid.ColumnDefinitions>
            <Grid Grid.Column="0" Panel.ZIndex="999">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="2*"/>
                    <ColumnDefinition Width="50"/>
                </Grid.ColumnDefinitions>
            </Grid>
            <Grid Grid.Column="1">
                <Grid>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
                        <Image Width="24">
                            <Image.Source>
                                <BitmapImage UriSource="lbc_pin_100.ce242dbebaf8.png"/>
                            </Image.Source>
                        </Image>
                        <TextBlock x:Name="TextBlockUsernameOwner" VerticalAlignment="Center" FontSize="15" Margin="6,0,6,0" Foreground="#777" FontWeight="Medium"/>
                        <Viewbox x:Name="TextbloxkusernameOwnerWarn" Width="16" Height="24" Visibility="Collapsed">
                            <Canvas Width="24" Height="24">
                                <Path Data="M13,14H11V10H13M13,18H11V16H13M1,21H23L12,2L1,21Z" Fill="Red" />
                            </Canvas>
                        </Viewbox>
                        <Button x:Name="ButtonUsernameOwner" Click="ButtonUsernameOwner_Click" Height="Auto" Width="28" VerticalAlignment="Center" Background="Transparent" BorderBrush="Transparent" 
                                        Style="{StaticResource TransparentStyle}" Visibility="Collapsed">
                            <Button.Content>
                                <Viewbox Width="20" Height="20" VerticalAlignment="Center" Margin="0,0,2.4,0">
                                    <Canvas Width="20" Height="24" >
                                        <Path Data="M19,12H22.32L17.37,16.95L12.42,12H16.97C17,10.46 16.42,8.93 15.24,7.75C12.9,5.41 9.1,5.41 6.76,7.75C4.42,10.09 4.42,13.9 6.76,16.24C8.6,18.08 11.36,18.47 13.58,17.41L15.05,18.88C12,20.69 8,20.29 5.34,17.65C2.22,14.53 2.23,9.47 5.35,6.35C8.5,3.22 13.53,3.21 16.66,6.34C18.22,7.9 19,9.95 19,12Z" 
                                                      Fill="Black" />
                                    </Canvas>
                                </Viewbox>
                            </Button.Content>
                            <Button.Resources>
                                <Style TargetType="{x:Type Border}">
                                    <Setter Property="CornerRadius" Value="20"/>
                                </Style>
                            </Button.Resources>
                        </Button>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                        <Button x:Name="ButtonNotifications" Cursor="Hand" Grid.Column="1" Width="45" BorderThickness="0" Style="{StaticResource Flat_Button}" Background="Transparent" Foreground="#777"
                            Click="ButtonNotifications_Click" Panel.ZIndex="999" Margin="0,0,20,0" HorizontalAlignment="Left">
                            <StackPanel Orientation="Horizontal">
                                <Viewbox Width="20" Height="24">
                                    <Canvas Width="24" Height="24">
                                        <Path Data="M9,22A1,1 0 0,1 8,21V18H4A2,2 0 0,1 2,16V4C2,2.89 2.9,2 4,2H20A2,2 0 0,1 22,4V16A2,2 0 0,1 20,18H13.9L10.2,21.71C10,21.9 9.75,22 9.5,22V22H9Z"
                                      Fill="{Binding Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}}" />
                                    </Canvas>
                                </Viewbox>
                                <Border x:Name="BorderNotifications" Visibility="Visible" CornerRadius="8" Width="16" Height="16" Background="#777" Panel.ZIndex="1" Margin="3,0,0,0">
                                    <TextBlock x:Name="TextBlockNotifications" Foreground="White" Padding="0" FontSize="14" Text="{Binding ElementName=ListBoxNotifications, Path=Items.Count}" FontWeight="Light" HorizontalAlignment="Center" Margin="2,-2,2,0"/>
                                </Border>
                            </StackPanel>
                        </Button>
                        <ListBox x:Name="ListBoxNotifications" Cursor="Hand" Margin="-320,34,-100,-179" Grid.Column="1" Visibility="Collapsed" MouseLeftButtonUp="ListBoxNotifications_MouseLeftButtonUp"
                         ScrollViewer.VerticalScrollBarVisibility="Disabled" HorizontalAlignment="Left" ScrollViewer.HorizontalScrollBarVisibility="Disabled" MinWidth="300" MaxWidth="300">
                            <ListBox.ItemTemplate>
                                <DataTemplate>
                                    <Grid>
                                        <TextBlock Text="{Binding}" TextWrapping="Wrap"/>
                                    </Grid>
                                </DataTemplate>
                            </ListBox.ItemTemplate>
                        </ListBox>
                        <StackPanel Orientation="Horizontal" Margin="0,0,20,0">
                            <Viewbox HorizontalAlignment="Left">
                                <Viewbox Width="29" Height="48">
                                    <Canvas Width="24" Height="24">
                                        <Path Data="M4.5,5H8V2H10V5H11.5V2H13.5V5C19,5 19,11 16,11.25C20,11 21,19 13.5,19V22H11.5V19H10V22H8V19H4.5L5,17H6A1,1 0 0,0 7,16V8A1,1 0 0,0 6,7H4.5V5M10,7V11C10,11 14.5,11.25 14.5,
                                      9C14.5,6.75 10,7 10,7M10,12.5V17C10,17 15.5,17 15.5,14.75C15.5,12.5 10,12.5 10,12.5Z" Fill="#777" />
                                    </Canvas>
                                </Viewbox>
                            </Viewbox>
                            <TextBlock x:Name="TextBlockBitcoinsBalance" TextAlignment="Center" VerticalAlignment="Center" Foreground="#777" Text="0" FontSize="15" />
                            <Viewbox x:Name="ViewboxWarnBitcoinsBalance" Width="16" Height="24" Visibility="Collapsed" Margin="5,0,0,0">
                                <Canvas Width="24" Height="24">
                                    <Path Data="M13,14H11V10H13M13,18H11V16H13M1,21H23L12,2L1,21Z" Fill="Red" />
                                </Canvas>
                            </Viewbox>
                            <Button x:Name="ButtonBitcoinsBalance" Click="ButtonBitcoinsBalance_Click" Height="Auto" Width="28" VerticalAlignment="Center" Background="Transparent" BorderBrush="Transparent" 
                                        Style="{StaticResource TransparentStyle}" Visibility="Collapsed">
                                <Button.Content>
                                    <Viewbox Width="20" Height="20" VerticalAlignment="Center" Margin="0,0,2.4,0">
                                        <Canvas Width="20" Height="24" >
                                            <Path Data="M19,12H22.32L17.37,16.95L12.42,12H16.97C17,10.46 16.42,8.93 15.24,7.75C12.9,5.41 9.1,5.41 6.76,7.75C4.42,10.09 4.42,13.9 6.76,16.24C8.6,18.08 11.36,18.47 13.58,17.41L15.05,18.88C12,20.69 8,20.29 5.34,17.65C2.22,14.53 2.23,9.47 5.35,6.35C8.5,3.22 13.53,3.21 16.66,6.34C18.22,7.9 19,9.95 19,12Z" 
                                                      Fill="Black" />
                                        </Canvas>
                                    </Viewbox>
                                </Button.Content>
                                <Button.Resources>
                                    <Style TargetType="{x:Type Border}">
                                        <Setter Property="CornerRadius" Value="20"/>
                                    </Style>
                                </Button.Resources>
                            </Button>
                        </StackPanel>
                        <Button Foreground="#777"  x:Name="ButtonVerification" Click="ButtonVerification_Click" FontFamily="Segoe UI Symbol" Margin="1,0,0,0" BorderThickness="0" 
                                Style="{StaticResource Flat_Button}" Background="Transparent">
                            <Button.Content>
                                <StackPanel Orientation="Horizontal">
                                    <Viewbox Width="20" Height="Auto" >
                                        <Canvas Width="24" Height="24">
                                            <Path Data="M2,3H22C23.05,3 24,3.95 24,5V19C24,20.05 23.05,21 22,21H2C0.95,21 0,20.05 0,19V5C0,3.95 0.95,3 2,3M14,6V7H22V6H14M14,8V9H21.5L22,9V8H14M14,10V11H21V10H14M8,13.91C6,13.91 2,15 2,17V18H14V17C14,15 10,13.91 8,13.91M8,6A3,3 0 0,0 5,9A3,3 0 0,0 8,12A3,3 0 0,0 11,9A3,3 0 0,0 8,6Z" 
                                                  Fill="{Binding Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}}">
                                            </Path>
                                        </Canvas>
                                    </Viewbox>
                                </StackPanel>
                            </Button.Content>
                        </Button>
                        <StackPanel Orientation="Horizontal" Margin="10,0">
                            <Button x:Name="ButtonSettings" Content="&#xe115;" Foreground="#777" FontSize="19"  FontFamily="Segoe UI Symbol" Margin="0,0,0,0" BorderThickness="0" Background="Transparent" 
                                    Click="ButtonSettings_Click" Style="{StaticResource Flat_Button }"/>
                            <Viewbox x:Name="WarnButtonSettingsPanel" Width="16" Height="24" Visibility="Collapsed">
                                <Canvas Width="24" Height="24">
                                    <Path Data="M13,14H11V10H13M13,18H11V16H13M1,21H23L12,2L1,21Z" Fill="Red" />
                                </Canvas>
                            </Viewbox>
                        </StackPanel>
                        <Button x:Name="ButtonHome" Style="{StaticResource Flat_Button}" Foreground="#777" Content="&#xe10f;" FontFamily="Segoe UI Symbol" FontSize="19" Grid.Column="2" BorderThickness="0" 
                                Background="Transparent" Click="ButtonHome_Click" Margin="0,0, 10, 0" >
                        </Button>
                    </StackPanel>
                    
                </Grid>
            </Grid>
        </Grid>
        <TabControl x:Name="tabControlBTC" TabStripPlacement="Left" Grid.Row="1" Grid.ColumnSpan="2" SelectionChanged="tabControlBTC_SelectionChanged">
            <TabControl.ItemContainerStyle>
                <Style TargetType="TabItem">
                    <Setter Property="Width" Value="240"/>
                    <Setter Property="Height" Value="40"/>
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="{x:Type TabItem}">
                                <Grid>
                                    <Border Width="Auto" Name="Border" Margin="3,0,0,0" CornerRadius="0" SnapsToDevicePixels="True">
                                        <ContentPresenter VerticalAlignment="Center" ContentSource="Header" Margin="12,2,12,2"/>
                                    </Border>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True" SourceName="Border">
                                    <Setter TargetName="Border" Property="Background" Value="#EDF5FA" />
                                    <Setter TargetName="Border" Property="BorderThickness" Value="4, 0, 0, 0"/>
                                    <Setter TargetName="Border" Property="BorderBrush" Value="DarkGray"/>
                                    <Setter TargetName="Border" Property="Margin" Value="3,0,0,0"/>
                                </Trigger>
                                <Trigger Property="IsSelected" Value="True">
                                    <Setter Property="Foreground" Value="Black"/>
                                    <Setter TargetName="Border" Property="Background">
                                        <Setter.Value>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="#DAE9F3" Offset="0.432" />
                                                <GradientStop Color="#DAE9F3" Offset="0.433" />
                                            </LinearGradientBrush>
                                        </Setter.Value>
                                    </Setter>
                                    <Setter TargetName="Border" Property="BorderThickness" Value="0"/>
                                    <Setter TargetName="Border" Property="BorderBrush" Value="DarkGray"/>
                                    <Setter TargetName="Border" Property="Margin" Value="0"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                </Style>
            </TabControl.ItemContainerStyle>
            <TabControl.Template>
                <ControlTemplate TargetType="TabControl">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <ScrollViewer HorizontalScrollBarVisibility="Disabled" Background="#F4F4F5" VerticalScrollBarVisibility="Hidden" FlowDirection="LeftToRight">
                            <TabPanel x:Name="HeaderPanel" Panel.ZIndex ="0" KeyboardNavigation.TabIndex="1" IsItemsHost="true"/>
                        </ScrollViewer>
                        <ContentPresenter x:Name="PART_SelectedContentHost" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" ContentSource="SelectedContent" Grid.Column="1"/>
                    </Grid>
                </ControlTemplate>
            </TabControl.Template>
            <TabItem x:Name="HomeTab" Margin="0,0" Visibility="Collapsed">
                <TabItem.Header>
                    <Grid>
                        <StackPanel Orientation="Horizontal" Opacity="1" Margin="-5,0,0,0" HorizontalAlignment="Left" Width="Auto">
                            <Viewbox Width="32" Height="32" Margin="0,0,4,0">
                                <Canvas Width="24" Height="24">
                                    <Path Data="M10,20V14H14V20H19V12H22L12,3L2,12H5V20H10Z" Fill="Black" />
                                </Canvas>
                            </Viewbox>
                            <StackPanel x:Name="HometabViewbox" VerticalAlignment="Top" Margin="0, -1, 0, 0" HorizontalAlignment="Center">
                                <TextBlock Text="Home" FontSize="15"></TextBlock>
                                <TextBlock Text="niceonne - 0 Trades open" FontSize="11"/>
                            </StackPanel>
                        </StackPanel>
                        <Button Background="Transparent" BorderThickness="0" HorizontalAlignment="Right" >
                            <Button.Content>
                                <Viewbox Width="15" Height="AUTO">
                                    <Canvas Width="24" Height="24">
                                        <Path Data="M13.46,12L19,17.54V19H17.54L12,13.46L6.46,19H5V17.54L10.54,12L5,6.46V5H6.46L12,10.54L17.54,5H19V6.46L13.46,12Z" Fill="Red" />
                                    </Canvas>
                                </Viewbox>
                            </Button.Content>
                        </Button>
                    </Grid>
                </TabItem.Header>
                <TabItem.Content>
                    <ScrollViewer CanContentScroll="True" VerticalScrollBarVisibility="Hidden">
                        <StackPanel x:Name="PanelHome" IsVisibleChanged="PanelHome_IsVisibleChanged" Visibility="Visible" HorizontalAlignment="Center">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0, 30, 0, 0" >
                                <TextBox x:Name="TextBoxHomeNewTrade" TextAlignment="Left" VerticalContentAlignment="Center" FontSize="15" Margin="10, 40, 10, 5" Width="230" Height="25">
                                    <TextBox.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="4"/>
                                        </Style>
                                    </TextBox.Resources>
                                </TextBox>
                                <Button x:Name="ButtonHomeViewTrade" Cursor="Hand" Click="ButtonHomeViewTrade_Click" Background="LightGray" Height="26" Width="120" Margin="0, 40, 10, 5" BorderThickness="1" BorderBrush="#ccc">
                                    <Button.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="4"/>
                                        </Style>
                                    </Button.Resources>
                                    <Button.Content>
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Text="View / Message" TextAlignment="Center" VerticalAlignment="Center" ></TextBlock>
                                            <Viewbox x:Name="WarnButtonViewTrade" Width="15" Height="18" Margin="5, 0, 0, 0" Visibility="Collapsed">
                                                <Canvas Width="24" Height="24">
                                                    <Path Data="M23,12L20.56,9.22L20.9,5.54L17.29,4.72L15.4,1.54L12,3L8.6,1.54L6.71,4.72L3.1,5.53L3.44,9.21L1,12L3.44,14.78L3.1,18.47L6.71,19.29L8.6,22.47L12,21L15.4,22.46L17.29,19.28L20.9,18.46L20.56,14.78L23,12M13,17H11V15H13V17M13,13H11V7H13V13Z" 
                                                      Fill="Red" />
                                                </Canvas>
                                                <Viewbox.ToolTip>
                                                    <StackPanel>
                                                        <TextBlock FontWeight="Bold">Error opening new trade!</TextBlock>
                                                        <TextBlock x:Name="TextBlockWarningViewTrade"/>
                                                    </StackPanel>
                                                </Viewbox.ToolTip>
                                            </Viewbox>
                                            <Viewbox x:Name="SpinnerButtonViewTrade" Visibility="Collapsed" Height="20" Width="20" Margin="2.5,0,0,0">
                                                <Spinners1:ucSpinnerApple />
                                            </Viewbox>
                                        </StackPanel>
                                    </Button.Content>
                                </Button>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Label FontSize="27" Content="Your open trades" FontWeight="SemiBold" Margin="0, 5" HorizontalContentAlignment="Left"/>
                                <Button x:Name="ButtonGridsTrades" Click="ButtonGridsTrades_Click"  Height="28" Width="28" Background="Transparent" BorderBrush="Transparent" Margin="0,7,0,0" 
                                        Style="{StaticResource TransparentStyle}" >
                                    <Button.Content>
                                        <Viewbox Width="20" Height="20" VerticalAlignment="Center" Margin="0,0,2.4,0">
                                            <Canvas Width="20" Height="24" >
                                                <Path Data="M19,12H22.32L17.37,16.95L12.42,12H16.97C17,10.46 16.42,8.93 15.24,7.75C12.9,5.41 9.1,5.41 6.76,7.75C4.42,10.09 4.42,13.9 6.76,16.24C8.6,18.08 11.36,18.47 13.58,17.41L15.05,18.88C12,20.69 8,20.29 5.34,17.65C2.22,14.53 2.23,9.47 5.35,6.35C8.5,3.22 13.53,3.21 16.66,6.34C18.22,7.9 19,9.95 19,12Z" 
                                                      Fill="Black" />
                                            </Canvas>
                                        </Viewbox>
                                    </Button.Content>
                                    <Button.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="20"/>
                                        </Style>
                                    </Button.Resources>
                                </Button>
                            </StackPanel>
                            <TextBlock x:Name="TextBlocktotalbitcoinsunderescrow" Text="Your total amount of bitcoins under escrow is 0 BTC." Margin="5, 0"/>
                            <Label Content="Selling bitcoins" FontSize="17" FontWeight="Medium" Margin="0,10,0,0" HorizontalContentAlignment="Left"/>
                            <DataGrid x:Name="myDataGridSelling" CanUserResizeColumns="True" Grid.Column="0" AutoGenerateColumns="False" BorderThickness="0, 1, 0, 0" BorderBrush="Gray" RowStyle="{StaticResource rowStyle}" 
                                   RowHeaderWidth="0" CanUserAddRows="False" GridLinesVisibility="Horizontal" >
                                <DataGrid.Resources>
                                    <Style TargetType="Hyperlink">
                                        <EventSetter Event="Click" Handler="DG_Hyperlink_Click"/>
                                    </Style>
                                </DataGrid.Resources>
                                <DataGrid.Columns>
                                    <DataGridHyperlinkColumn x:Name="contactidColumn" Header="{Binding HeaderName}" Binding="{Binding ContactNumber, Mode=OneWay}" Width="75" IsReadOnly="True">
                                        <DataGridHyperlinkColumn.HeaderTemplate>
                                            <DataTemplate>
                                                <StackPanel>
                                                    <TextBlock Text="#" Margin="0, 2" FontWeight="Bold"/>
                                                </StackPanel>
                                            </DataTemplate>
                                        </DataGridHyperlinkColumn.HeaderTemplate>
                                    </DataGridHyperlinkColumn>
                                    <DataGridTextColumn x:Name="tradecreatedatColumn" FontWeight="Bold" Header="{Binding HeaderName}" Binding="{Binding CreatedAt, Mode=OneWay}" Width="130" IsReadOnly="True">
                                        <DataGridTextColumn.HeaderTemplate>
                                            <DataTemplate>
                                                <StackPanel>
                                                    <TextBlock Text="Created at" FontWeight="Bold" Margin="0, 2"/>
                                                </StackPanel>
                                            </DataTemplate>
                                        </DataGridTextColumn.HeaderTemplate>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn x:Name="usernameColumn" Header="{Binding HeaderName}" Binding="{Binding Nickname, Mode=OneWay}" Width="100" IsReadOnly="True">
                                        <DataGridTextColumn.HeaderTemplate>
                                            <DataTemplate>
                                                <StackPanel>
                                                    <TextBlock Text="Buyer" FontWeight="Bold" Margin="0, 2"/>
                                                </StackPanel>
                                            </DataTemplate>
                                        </DataGridTextColumn.HeaderTemplate>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn x:Name="amountBTCColumn" Binding="{Binding AmountBtc, Mode=OneWay}" Header="{Binding HeaderName}" Width="90" IsReadOnly="True">
                                        <DataGridTextColumn.HeaderTemplate>
                                            <DataTemplate>
                                                <StackPanel>
                                                    <TextBlock Text="Amount (BTC)" FontWeight="Bold" Margin="0, 2"/>
                                                </StackPanel>
                                            </DataTemplate>
                                        </DataGridTextColumn.HeaderTemplate>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn x:Name="amountColumn" Binding="{Binding Amount, Mode=OneWay}" Header="{Binding HeaderName}" Width="90" IsReadOnly="True">
                                        <DataGridTextColumn.HeaderTemplate>
                                            <DataTemplate>
                                                <StackPanel>
                                                    <TextBlock Text="Amount (fiat)" FontWeight="Bold"  Margin="0, 2"/>
                                                </StackPanel>
                                            </DataTemplate>
                                        </DataGridTextColumn.HeaderTemplate>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn x:Name="paymentmethodColumn" Binding="{Binding PaymentMethod, Mode=OneWay}" Header="{Binding HeaderName}" Width="100" IsReadOnly="True">
                                        <DataGridTextColumn.HeaderTemplate>
                                            <DataTemplate>
                                                <StackPanel>
                                                    <TextBlock Text="Type" FontWeight="Bold"  Margin="0, 2"/>
                                                </StackPanel>
                                            </DataTemplate>
                                        </DataGridTextColumn.HeaderTemplate>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn x:Name="tradestatusColumn" Binding="{Binding Status, Mode=OneWay}" Header="{Binding HeaderName}" Width="150" IsReadOnly="True">
                                        <DataGridTextColumn.HeaderTemplate>
                                            <DataTemplate>
